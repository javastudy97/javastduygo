package java1124;

public class Test1 {
	public static void main(String[] args) {
		
		
		int index=2;
		while(index<10){
		       System.out.println(index+"단입니다.");
		        int j=1;
		        while(j<10){
		            System.out.println(index+"*"+j+"="+(index*j));
		            j++;
		        }
		 index++;
		} 
		
	}
	
}
