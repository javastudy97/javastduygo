package org.study.thread;

//1. 공유 자원 Synchronize
class ShareStorage{
	
	//Thread공유자원
	synchronized void storagePrint(String str) {
		System.out.println(str);
		
	}
}
//2. Thread -> 공유 자원 Synchronize 접근

